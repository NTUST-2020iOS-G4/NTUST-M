  pod 'Alamofire', '~> 5.1'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'FSCalendar'
  pod 'KeychainSwift', '~> 19.0'
  pod 'BTNavigationDropdownMenu'

end
